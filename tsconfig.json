{
	"compilerOptions": {
		"outDir": "dist",
		"sourceMap": true,
		"target": "esnext",
		"module": "CommonJS",
		"noEmit": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"strict": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"strictPropertyInitialization": false,
		"noUncheckedIndexedAccess": true,
		"noImplicitReturns": true
	}
}
